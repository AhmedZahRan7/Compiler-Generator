{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:/MinGW/bin/g++.exe",
			"args": [
				"-g",
				"${workspaceFolder}\\DFA.cpp",
				"${workspaceFolder}\\main.cpp",
				"${workspaceFolder}\\NFA.cpp",
				"${workspaceFolder}\\State.cpp",
				"${workspaceFolder}\\Token.cpp",
				"${workspaceFolder}\\TokenKey.cpp",
				"${workspaceFolder}\\TokenValue.cpp",
				"${workspaceFolder}\\Transation.cpp",
				"${workspaceFolder}\\utilities.cpp",
				"-o",
				"${fileDirname}\\run.exe"
			],
			"options": {
				"cwd": "C:/MinGW/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: C:/MinGW/bin/g++.exe"
		}
	]
}